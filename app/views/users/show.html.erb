<section class="right">
	<h1> Your questions </h1>
		<% @questions.each do |question| %> 
			<div>
				<h2><%= link_to question.title, question_path(question) %> 
					<small> written <%= time_ago_in_words(question.created_at)%>  ago  
					</small>
				</h2>
					<% question.tags.each do |tag| %>  
				 		<small> <%= link_to "##{tag.content}", tag_path(tag) %>  </small>
					<% end %>  
			 </div>
		<% end %>
</section> 